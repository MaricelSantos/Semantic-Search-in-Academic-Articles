journal = {Renewable Energy},
keywords = {Biodiesel,Catalyst,Deep eutectic solvent,Esterification,Free fatty acids,Glycerol},
pages = {480--488},
publisher = {Elsevier Ltd},
title = {{Application of deep eutectic solvents as catalysts for the esterification of oleic acid with glycerol}},
url = {http://dx.doi.org/10.1016/j.renene.2017.07.046},
