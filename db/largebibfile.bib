pages = {172--180},
publisher = {Elsevier B.V.},
title = {{Enzymatic conversion of corn oil into biodiesel in a batch supercritical carbon dioxide reactor and kinetic modeling}},
url = {http://dx.doi.org/10.1016/j.supflu.2012.12.029},
