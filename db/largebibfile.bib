keywords = {Beta-cyclodextrin,Diglyceride,Esterification,Lipase,Monoglyceride},
number = {3-4},
pages = {157--162},
publisher = {Elsevier B.V.},
title = {{Optimization of Candida sp. 99-125 lipase catalyzed esterification for synthesis of monoglyceride and diglyceride in solvent-free system}},
