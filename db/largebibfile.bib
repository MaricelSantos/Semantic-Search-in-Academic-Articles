issn = {03783820},
journal = {Fuel Processing Technology},
keywords = {Acid oils,Biodiesel feedstock,Glycerolysis,Soap-stocks},
number = {6},
pages = {1225--1229},
publisher = {Elsevier B.V.},
title = {{Study on the glycerolysis reaction of high free fatty acid oils for use as biodiesel feedstock}},
url = {http://dx.doi.org/10.1016/j.fuproc.2011.01.020},
volume = {92},
year = {2011}
}
