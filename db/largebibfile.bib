issn = {13811177},
journal = {Journal of Molecular Catalysis B: Enzymatic},
keywords = {AoL,Aspergillus oryzae lipase,CALB,Candida antarctica lipase B,Candida cylindracea lipase,Candida lipolytica lipase,CcL,ClL,MjL,Mucor javanicus lipase,PPL,PcL,Penicillium roqueforti lipase,PrL,Rhizomucor miehei lipase,RmL,porcine pancreas lipase},
number = {1-6},
pages = {9--12},
title = {{Kinetic and modelling studies on the lipase catalysed enantioselective esterification of (±)-perillyl alcohol}},
volume = {29},
year = {2004}
}
@article{Xia2003,
abstract = {Enzymatic synthesis of partial glycerol caprate from capric acid and glycerol was investigated by using lipase from Candida antarctica (CAL) in a solvent-free microaqueous media. In a closed reactor, equilibrium conversion reached 65.6% in 9h under optimal reaction conditions which were: temperature, 60°C; initial water content in glycerol, 12% (w/w); lipase dosage, 100U/g of capric acid; molar ratio of glycerol:capric acid, 1:1. Initial reaction mixture phase states were investigated by using a flat plate cooling-quenching method with a micrograph system. Regional capric acid concentration surrounding the immobilized lipase surface is higher than that in the bulk phase. CAL did not express 1,3-position specificity in the final product. Mechanical fray partly denatured CAL in batch reactor. Capric acid conversion as high as 96.9% was obtained in 6h with several modified reaction procedures in a batch reactor. {\textcopyright} 2003 Elsevier Science B.V. All rights reserved.},
